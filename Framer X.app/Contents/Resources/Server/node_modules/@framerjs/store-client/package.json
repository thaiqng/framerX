{
  "name": "@framerjs/store-client",
  "version": "0.8.0",
  "description": "An HTTP client to Framer's Store API",
  "author": "fhur",
  "license": "UNLICENCED",
  "repository": "@framerjs/store-client",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "test": "yarn run test:ci",
    "test:ci": "cross-env CI=1 react-scripts-ts test --env=jsdom",
    "test:watch": "react-scripts-ts test --env=jsdom",
    "lint:ci": "tslint -c tslint.json --project ./",
    "prettier": "prettier --config .prettierrc --write '*.json' './src/**/*.ts'",
    "prettier:ci": "prettier --config .prettierrc --list-different '*.json' './src/**/*.ts'",
    "build": "rollup -c",
    "start": "rollup -c -w",
    "prepare": "yarn run build",
    "pub": "yarn publish --no-git-tag-version --minor --message \"Bumping @framerjs/store-client minor from version $npm_package_version [skip ci]\"",
    "version:snapshot": "semver -i patch $(semver -i patch $npm_package_version)",
    "pub:snapshot": "yarn publish --no-git-tag-version --no-commit-hooks --new-version \"$(yarn run --silent version:snapshot)-SNAPSHOT\""
  },
  "dependencies": {
    "request": "^2.88.0",
    "request-promise-native": "^1.0.7"
  },
  "devDependencies": {
    "@types/jest": "^23.1.5",
    "@types/lodash.merge": "^4.6.6",
    "@types/react": "^16.3.13",
    "@types/react-dom": "^16.0.5",
    "@types/request-promise-native": "^1.0.16",
    "babel-core": "^6.26.3",
    "babel-runtime": "^6.26.0",
    "cross-env": "^5.1.4",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-scripts-ts": "^2.16.0",
    "rollup": "^0.62.0",
    "rollup-plugin-auto-external": "^2.0.0",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-typescript2": "^0.17.0",
    "typescript": "^2.8.3"
  },
  "files": [
    "dist"
  ]
}
